<?xml version="1.0" encoding="UTF-8"?>
<!--
	Value mapping master definition. Use this to include value maps that are to be
	processed by the VLO.
-->
<value-mappings xmlns:xi="http://www.w3.org/2001/XInclude">

	<!-- `_componentProfile` to `resourceClass` -->
	<xi:include 
		href="toolportal_profile-resourceClass.xml" 
	  	xpointer="element(/1/1)" />

	<!-- `resourceClass` to `resourceClass` -->
	<xi:include 
		href="toolportal_resourceClass.xml" 
	  	xpointer="element(/1/1)" />

	<!-- `toolType` to `toolType` and `resourceClass` -->
	<xi:include 
		href="toolportal_toolType.xml" 
	  	xpointer="element(/1/1)" />

	<!-- `keywords` to `task` -->
	<xi:include 
		href="toolportal_keywords-task.xml" 
	  	xpointer="element(/1/1)" />

	<!-- `task` to `task` -->
	<xi:include 
		href="toolportal_task.xml" 
	  	xpointer="element(/1/1)" />

	<!-- `fieldOfStudy` to `fieldOfStudy` -->
	<xi:include 
		href="toolportal_fieldOfStudy.xml" 
	  	xpointer="element(/1/1)" />
	  		  	
	<!-- `collection` to `collection` -->
	<xi:include 
		href="toolportal_collection.xml" 
		xpointer="element(/1/1)" />

</value-mappings>
